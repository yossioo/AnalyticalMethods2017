\begin_layout Standard
\begin_inset Formula 
\[
b_{1}=b_{0}\frac{3+0\left(0-1\right)}{0+1}=3b_{0}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
b_{2}=\frac{b_{1}\left[3+\left(0+2-1\right)\left(0+2-2\right)\right]+6b_{0}\left(0+2-2\right)}{\left(0+2\right)\left(0+2-1\right)}=\frac{3}{2}b_{1}=\frac{9}{2}b_{1}
\]

\end_inset


\end_layout

\begin_layout Subsection
